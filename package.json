{
  "name": "app-devtools",
  "version": "0.12.0",
  "description": "",
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "test:run": "echo 'no tests'",
    "preview": "vite preview",
    "tsc": "tsc -p tsconfig.prod.json",
    "eslint": "eslint --ext .js,.ts,.tsx,.jsx src",
    "lint": "pnpm eslint && pnpm tsc",
    "tsc:w": "tsc -w -p tsconfig.prod.json",
    "convert-har": "tsm scripts/filterFetchRequests.ts",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test",
    "build:no-test": "vite build && tsup --dts-only",
    "deploy": "./scripts/check-if-is-sync.sh && pnpm run build && pnpm publish"
  },
  "packageManager": "pnpm@6.29.1",
  "license": "MIT",
  "author": "Lucas Santos",
  "repository": "github:lucasols/app-devtools",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "dequal": "^2.0.3",
    "diff": "^5.1.0",
    "klona": "^2.0.6",
    "nanoid": "^4.0.1",
    "regexparam": "^2.0.1",
    "solid-js": "^1.6.11",
    "solid-styled-components": "^0.28.5",
    "tinykeys": "^1.4.0",
    "typescript-styled-plugin": "^0.18.2"
  },
  "volta": {
    "node": "16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@lucasols/eslint-plugin-extended-lint": "^0.1.1",
    "@types/diff": "^5.0.2",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/ui": "^0.28.5",
    "babel-plugin-solid-labels": "^0.14.2",
    "eslint": "^8.34.0",
    "happy-dom": "^8.7.1",
    "oslu": "^0.9.0",
    "tsm": "^2.3.0",
    "tsup": "^6.6.3",
    "typescript": "4.9.5",
